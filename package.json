  "dependencies": {
    "aws-sdk": "^2.338.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "http-proxy-middleware": "^0.19.0",
    "massive": "^5.5.0",
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-burger-menu": "^2.5.4",
    "react-dom": "^16.5.2",
    "react-dropzone": "^7.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-slick": "^0.23.1",
    "react-spinners": "^0.4.7",
